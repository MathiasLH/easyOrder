<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.MainFragment"
    tools:layout_editor_absoluteY="25dp">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="433dp"
        app:srcCompat="@drawable/smorrebrod"
        tools:layout_editor_absoluteY="8dp"
        tools:src="@tools:sample/avatars[0]" />

    <Switch
        android:id="@+id/switch2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="Lyst eller mørkt brød"
        android:text="Lyst"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="213dp"
        tools:layout_editor_absoluteY="476dp" />

    <Switch
        android:id="@+id/switch3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="true"
        android:text="Switch"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="518dp" />

    <Switch
        android:id="@+id/switch4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="true"
        android:text="Switch"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="630dp" />

    <Switch
        android:id="@+id/switch5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="true"
        android:text="Switch"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="574dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lyst eller Mørks brød"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        tools:layout_editor_absoluteY="475dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="emne1"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="527dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="emne2"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="579dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="emne3"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="632dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="235dp"
        android:layout_height="wrap_content"
        android:text="Bestil"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="684dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mørkt"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="476dp" />

</android.support.constraint.ConstraintLayout>
